{
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    }
  },
  "Service": {
    "Name": "shoppingcart-api"
  },
  "ApplicationInsights": {
    "ConnectionString": ""
  },
  "Seq": {
    "ServerUrl": ""
  },
  "HealthCheckHostedService": {
    "Name": "{{Service:Name}}",
    "Enabled": true,
    "Interval": 5,
    "CacheDuration": 30,
    "Checks": [
      {
        "Name": "shoppingcart-db",
        "Type": "dbcontext",
        "Required": true,
        "Interval": 30,
        "Timeout": 5
      },
      {
        "Name": "policyserver",
        "Type": "url",
        "Required": false,
        "Value": "{{PolicyServer:PolicyServerUrl}}/health",
        "Interval": 30,
        "Timeout": 5
      },
      {
        "Name": "identityserver",
        "Type": "url",
        "Required": false,
        "Value": "{{IdentityServer:Authority}}/api/health",
        "Interval": 30,
        "Timeout": 5
      },
      {
        "Name": "example",
        "Type": "example",
        "Required": false,
        "Interval": 30,
        "Timeout": 5
      },
      {
        "Name": "catalog-api",
        "Type": "url",
        "Required": false,
        "Value": "{{CatalogApi:ServiceUrl}}/api/health",
        "Interval": 30,
        "Timeout": 5
      }
    ]
  },
  "ExampleHostedService": {
    "Enabled": true,
    "Interval": 60
  },
  "Database": {
    "ConnectionString": "Data Source=localhost;Initial Catalog=ShoppingCart;Integrated Security=True",
    "DatabaseConnectionString" : "Server=localhost;Database=admin;Port=5431;User Id=postgres;Password=master"
  },
  "ServiceBus": {
    "Protocol": "amqp",
    "Namespace": "localhost",
    "Policy": "admin",
    "Key": "password",
    "Queue": "max.queue",
    "Topic": "/exchange/max/",
    "Credits": 5
  },
  "IdentityServer": {
    "Authority": "http://localhost:50002",
    "ApiName": "max-api",
    "ApiSecret": "secret",
    "RequireHttpsMetadata": false,
    "CacheDuration": "00:10:00",
    "CacheEnabled": true,
    "EnableCaching": true,
    "Authentication": {
      "GrantType": "client_credentials",
      "Scope": "policyserver.runtime catalog-api",
      "ClientId": "max-service",
      "ClientSecret": "secret",
      "SlidingExpiration": 30
    }
  },
  "PolicyServer": {
    "CacheDuration": "0.00:05:00",
    "BasePolicy": "ShoppingCart",
    "PolicyServerUrl": "http://localhost:50001",
    "BasePolicyPrefix": "ShoppingCart"
  },
  "ReceiverHostedService": {
    "Enabled": true,
    "TimedInterval": 60
  },
  "Cors": {
    "Origins": [
      "http://*.cortside.local",
      "https://*.cortside.com",
      "http://localhost:4200",
      "https://localhost:4200"
    ]
  },
  "OutboxHostedService": {
    "BatchSize": 5,
    "Enabled": true,
    "Interval": 5,
    "PurgePublished": false
  },
  "CatalogApi": {
    "ServiceUrl": "http://localhost:5001"
  },
  "Encryption": {
    "Secret": "*G-KaPdSgVkYp3s6v8y/B?E(H+MbQeTh"
  },
  "Bowdlerizer": [
    {
      "Strategy": "head",
      "Path": "$..Customer.FirstName",
      "Length": 0
    },
    {
      "Strategy": "head",
      "Path": "$..Customer.LastName",
      "Length": 0
    }
  ]
}
